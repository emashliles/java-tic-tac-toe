<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import java.net.URL?>
<GridPane
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10" fx:controller="PlayerOptionsController" style="root">
    <Button text="Human v Human" onAction="#HumanvHuman" fx:id="hvh" GridPane.rowIndex="1"/>
    <Button text="Human v Computer" onAction="#HumanVComputer" fx:id="hvc" GridPane.rowIndex="2"/>
    <stylesheets>
        <URL value="@TTTStyle.css" />
    </stylesheets>
</GridPane>